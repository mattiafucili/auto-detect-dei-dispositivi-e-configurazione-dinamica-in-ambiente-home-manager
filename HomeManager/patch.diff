+            ht.put(2, "'Tuesday'");
+            ht.put(3, "'Wednesday'");
+            ht.put(4, "'Thursday'");
+            ht.put(5, "'Friday'");
+            ht.put(6, "'Saturday'");
+            ht.put(7, "'Sunday'");
