+        Vector cmds = new Vector();
+        try {
+            /*LogicTuple listTuples = inp(myTid,
+                    new LogicTuple("in_all",new Value("send_cmd",new Var("W"), // user id
+                            room, // room
+                            new Var("Y"), // device
