+        int fM = Integer.parseInt(st.nextToken());
+        StringTokenizer st2 = new StringTokenizer(tHour, ":");
+        int tH = Integer.parseInt(st2.nextToken());
+        int tM = Integer.parseInt(st2.nextToken());
